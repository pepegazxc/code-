import java.util.Scanner;

public class Main {
    public static void main(String[] args) {

        Scanner sc = new Scanner(System.in);
        System.out.println("Введите большее и меньшее число  :");
        System.out.println();

        int a = sc.nextInt();
        int b = sc.nextInt();

        System.out.println("Числа которые вы ввели:" + a + "," + b);


        if (a % b == 0) {
            System.out.println("Первое число кратно второму");
        } else {
            System.out.println(a + " не кратно " + b + ",остаток от деления:" + a % b);
        }

    }
}
